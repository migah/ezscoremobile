<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pineapple.ezscore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ezscorelogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ezscorelogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MatchActivity" />
        <activity android:name=".NewMatchActivity" />
        <activity android:name=".MyMatchesActivity" />
        <activity android:name=".MatchEditActivity"></activity>
    </application>

</manifest>